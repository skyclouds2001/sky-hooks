{
  "name": "@sky-fly/sky-hooks",
  "version": "0.0.3",
  "license": "MIT",
  "description": "A useful vue composition function library",
  "keywords": [
    "vue",
    "composition function",
    "library"
  ],
  "files": [
    "dist",
    "type"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "browser": "./dist/index.umd.js",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "browser": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://github.com/skyclouds2001/sky-hooks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyclouds2001/sky-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/skyclouds2001/sky-hooks/issues",
    "email": "skyclouds2001@163.com"
  },
  "author": {
    "name": "skyclouds2001",
    "email": "skyclouds2001@163.com",
    "url": "https://skyclouds2001.github.io"
  },
  "contributors": [
    "skyclouds2001"
  ],
  "packageManager": "pnpm@8.4.0",
  "engines": {
    "node": ">=18.13.0",
    "npm": ">=9.6.0",
    "yarn": ">=1.22.0",
    "pnpm": ">=8.4.0"
  },
  "scripts": {
    "build": "rollup --config rollup.config.js --bundleConfigAsCjs",
    "lint": "eslint --fix . && prettier --write . && pnpm stylelint --fix . && markdownlint --fix **/*.md && tsc --noEmit",
    "test": "vitest",
    "clean": "rimraf node_modules && rimraf dist",
    "changeset": "changeset",
    "bump": "changeset version",
    "commit": "cz",
    "prepare": "is-ci || husky install",
    "preinstall": "only-allow pnpm"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-typescript": "^7.22.5",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitest/coverage-v8": "0.32.0",
    "@vitest/ui": "^0.32.2",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.4.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-regexp": "^1.15.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vitest": "^0.2.6",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "markdownlint": "^0.29.0",
    "markdownlint-cli": "^0.35.0",
    "only-allow": "^1.1.1",
    "pnpm": "8.4.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "stylelint": "^15.9.0",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.1.5",
    "vitest": "^0.32.2"
  }
}
